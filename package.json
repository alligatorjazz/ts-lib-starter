{
	"name": "ts-lib-starter",
	"version": "0.1.0rc1",
	"description": "A starter repo for new typescript libraries.",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "vitest --run",
		"check": "tsc --noEmit",
		"dev": "esrun --watch dist/index.js",
		"start": "node dist/index.js"
	},
	"pre-commit": [
		"check",
		"test"
	],
	"author": "Falchion Studios",
	"license": "UNLICENSED",
	"devDependencies": {
		"esrun": "^3.2.26",
		"pre-commit": "^1.2.2",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	},
	"files": [
		"dist"
	],
	"types": "dist/index.d.ts"
}
